  "supportURL": "https://github.com/baturkacamak/user-scripts/issues",
  "downloadURL": "https://github.com/baturkacamak/user-scripts/raw/master/userscripts/tidal-uri-scheme-converter/tidal-uri-scheme-converter.user.js",
  "updateURL": "https://github.com/baturkacamak/user-scripts/raw/master/userscripts/tidal-uri-scheme-converter/tidal-uri-scheme-converter.user.js",
